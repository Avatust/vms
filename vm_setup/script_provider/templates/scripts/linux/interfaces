# Source additional interfaces
source /etc/network/interfaces.d/*

# The loopback network interface
auto lo
iface lo inet loopback

# The primary network interface
auto "$ETHERNET_IFNAME"
iface "$ETHERNET_IFNAME" inet static
  address 10.114.48.$HOST_NUM
  netmask 255.255.255.0
  network 10.114.48.0
  broadcst 10.114.48.255
  gateway 10.114.48.1
  dns-nameserver 193.167.197.100
  dns-nameserver 195.148.144.100
